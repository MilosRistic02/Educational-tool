<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="823.0" prefWidth="1172.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.tudelft.oopp.demo.controllers.QuestionController">
   <children>
      <Button layoutX="910.0" layoutY="362.0" mnemonicParsing="false" onAction="#displayQuestion" prefHeight="31.0" prefWidth="49.0" style="-fx-background-color: #00A6D6;" text="Send" textFill="WHITE" />
      <TextField fx:id="questionText" layoutX="910.0" layoutY="277.0" prefHeight="74.0" prefWidth="238.0" promptText="Write your question here." />
      <Button layoutX="972.0" layoutY="362.0" mnemonicParsing="false" onAction="#displayAllQuestion" prefHeight="31.0" prefWidth="49.0" style="-fx-background-color: #00A6D6;" text="Show" textFill="WHITE" />
      <Button layoutX="1055.0" layoutY="692.0" mnemonicParsing="false" onAction="#closeRoom" prefHeight="31.0" prefWidth="93.0" style="-fx-background-color: #00A6D6;" text="Close room" textFill="WHITE" />
      <Pane layoutX="13.0" layoutY="25.0" prefHeight="699.0" prefWidth="876.0" style="-fx-background-color: #00A6D6; -fx-background-radius: 18;">
         <children>
            <Text fill="WHITE" layoutY="49.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Questions" textAlignment="CENTER" wrappingWidth="876.0">
               <font>
                  <Font size="50.0" />
               </font>
            </Text>
            <ScrollPane hbarPolicy="NEVER" layoutY="75.0">
               <content>
                  <VBox fx:id="stack" alignment="CENTER" prefHeight="607.0" prefWidth="874.0" style="-fx-background-color: #FFFFFF;" />
               </content>
            </ScrollPane>
         </children>
      </Pane>
      <Text fx:id="greetings" layoutX="910.0" layoutY="64.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome, [username], you are in: [lectureroom]" textAlignment="CENTER" wrappingWidth="238.0">
         <font>
            <Font size="36.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
