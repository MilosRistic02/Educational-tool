<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.tudelft.oopp.demo.controllers.LobbyController">
   <children>
      <Label layoutX="100.0" layoutY="122.0" prefHeight="64.0" prefWidth="398.0" text="Enter a lecture pin to join a lecture" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="pinText" layoutX="165.0" layoutY="207.0" prefHeight="40.0" prefWidth="269.0" promptText="Your pin here" />
      <Button layoutX="230.0" layoutY="274.0" mnemonicParsing="false" onAction="#checkPin" prefHeight="64.0" prefWidth="138.0" text="Join" textFill="WHITE">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
