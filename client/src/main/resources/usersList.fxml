<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.tudelft.oopp.demo.controllers.UsersListController">
    <children>
        <Pane layoutY="20.0" prefHeight="699.0" prefWidth="876.0" style="-fx-background-color: #00A6D6; -fx-background-radius: 18;">
            <children>
                <Text fx:id="title" fill="WHITE" layoutY="49.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Unbanned users" textAlignment="CENTER" wrappingWidth="876.0">
                    <font>
                        <Font size="40.0" />
                    </font>
                </Text>
                <ScrollPane hbarPolicy="NEVER" layoutY="75.0">
                    <content>
                        <VBox fx:id="stack" alignment="CENTER" prefHeight="607.0" prefWidth="874.0" style="-fx-background-color: #FFFFFF;">
                     <children>
                     </children></VBox>
                    </content>
                </ScrollPane>
                <ImageView fitHeight="45.0" fitWidth="54.0" layoutX="10.0" layoutY="12.0" onMouseClicked="#loadLobbyLecturer" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@images/back_button_white.png" />
                    </image>
                </ImageView>
                <Label fx:id="userListEmptyText" alignment="BOTTOM_CENTER" contentDisplay="CENTER" layoutX="286.0" layoutY="-212.0" prefHeight="509.0" prefWidth="304.0" text="Users List is Empty!" visible="false">
                    <font>
                        <Font size="30.0" />
                    </font>
                </Label>
            <TextField fx:id="enterUserField" layoutX="614.0" layoutY="21.0" onKeyTyped="#searchForUser" prefHeight="28.0" prefWidth="190.0" promptText="Search for user..." />
            <Button fx:id="switchViewButton" layoutX="121.0" layoutY="23.0" mnemonicParsing="false" onAction="#switchView" text="Banned Users" />
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="704.0" layoutY="-10.0" pickOnBounds="true" preserveRatio="true" />
            </children>
        </Pane>
    </children>

</AnchorPane>
